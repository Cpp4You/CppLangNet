{
	"extends": "@tsconfig/docusaurus/tsconfig.json",
	"exclude": [
		".docusaurus/*",
		"**/node_modules/",
		"build"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"types": [
			"node"
		],
		"paths": {
			"@site/*": [
				"./*"
			],
			"@site-comps/*": [
				"./src/components/mdx/*"
			]
		}
	},
}